<Tab closable="false" disable="true" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.dimensions.DimensionsController">
	<graphic>
		<BorderPane>
			<center>
				<ImageView fx:id="image" pickOnBounds="true"
					preserveRatio="true" BorderPane.alignment="CENTER">
					<image>
						<Image url="images/dimension_black.png" />
					</image>
				</ImageView>
			</center>
			<bottom>
				<Label text="Dimensions" BorderPane.alignment="CENTER" />
			</bottom>
		</BorderPane>
	</graphic>
	<content>
		<ScrollPane>
			<content>
				<HBox>
					<children>
						<AnchorPane>
							<children>
								<Label layoutX="45.0" layoutY="26.0" text="Available Dimensions"
									textAlignment="CENTER" AnchorPane.leftAnchor="45.0"
									AnchorPane.topAnchor="26.0" />
								<ListView layoutX="2.0" layoutY="71.0" prefHeight="400.0"
									prefWidth="210.0" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="70.0" />
								<Button layoutX="13.0" layoutY="480.0" mnemonicParsing="false"
									text="Add dimension" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" />
								<Button layoutX="10.0" layoutY="520.0" mnemonicParsing="false"
									text="Remove dimension" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" />
							</children>
						</AnchorPane>
						<AnchorPane prefHeight="480.0" prefWidth="307.0">
							<children>
								<Label layoutX="50.0" layoutY="14.0" text="Attribute Hierarchy"
									AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="14.0" />
								<ListView layoutX="14.0" layoutY="70.0" prefHeight="400.0"
									prefWidth="210.0" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="92.0" AnchorPane.topAnchor="70.0" />
								<Label layoutX="50.0" layoutY="35.0" text="(of selected Dimension)"
									AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="35.0">
									<font>
										<Font size="12.0" />
									</font>
								</Label>
								<Button contentDisplay="TOP" layoutX="233.0" layoutY="155.0"
									mnemonicParsing="false" prefHeight="85.0" prefWidth="56.0"
									text="Move Up" textAlignment="CENTER" wrapText="true">
									<graphic>
										<ImageView pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="/images/arrow_up.png" />
											</image>
										</ImageView>
									</graphic>
									<font>
										<Font size="11.0" />
									</font>
								</Button>
								<Button contentDisplay="BOTTOM" layoutX="233.0" layoutY="301.0"
									mnemonicParsing="false" prefHeight="85.0" prefWidth="56.0"
									text="Move Down" textAlignment="CENTER" wrapText="true">
									<font>
										<Font size="11.0" />
									</font>
									<graphic>
										<ImageView pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="/images/arrow_down.png" />
											</image>
										</ImageView>
									</graphic>
								</Button>
								<Label layoutX="233.0" layoutY="70.0" prefHeight="42.0"
									prefWidth="57.0" text="Lowest Granularity" textAlignment="CENTER"
									wrapText="true">
									<font>
										<Font size="11.0" />
									</font>
								</Label>
								<Label layoutX="233.0" layoutY="428.0" prefHeight="42.0"
									prefWidth="57.0" text="Highest Granularity" textAlignment="CENTER"
									wrapText="true">
									<font>
										<Font size="11.0" />
									</font>
								</Label>
								<Separator layoutX="304.0" orientation="VERTICAL"
									prefHeight="552.0" prefWidth="5.0" />
								<Button layoutX="80.0" layoutY="482.0" mnemonicParsing="false"
									prefHeight="31.0" prefWidth="205.0" text="Add attribute"
									AnchorPane.leftAnchor="10.0" />
								<Button layoutX="10.0" layoutY="518.0" mnemonicParsing="false"
									prefHeight="31.0" prefWidth="205.0" text="Remove attribute"
									AnchorPane.leftAnchor="10.0" />
							</children>
						</AnchorPane>
						<AnchorPane>
							<children>
								<Label layoutX="51.0" layoutY="26.0" text="Unused attributes"
									AnchorPane.leftAnchor="51.0" AnchorPane.topAnchor="26.0" />
								<ListView fx:id = "unusedAttributes" layoutX="14.0" layoutY="71.0" prefHeight="400.0"
									prefWidth="200.0" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" />
							</children>
						</AnchorPane>
					</children>
				</HBox>
			</content>
		</ScrollPane>
	</content>
</Tab>

