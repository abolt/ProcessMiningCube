<Tab closable="false" disable="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.materialize.MaterializeController">
	<graphic>
		<BorderPane>
			<center>
				<ImageView fx:id="image" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
					<image>
						<Image url="images/gear_black.png" />
					</image>
				</ImageView>
			</center>
			<bottom>
				<Label text="Materialize" BorderPane.alignment="CENTER" />
			</bottom>
		</BorderPane>
	</graphic>
	<content>
		<ScrollPane>
			<content>
				<BorderPane>
					<left>
						<AnchorPane prefHeight="645.0" prefWidth="196.0" BorderPane.alignment="CENTER" >
							<children>
								<Button onAction = "#exportSelected" layoutX="10.0" layoutY="524.0" mnemonicParsing="false" text="Export Selected Cells" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Button onAction = "#selectAll" layoutX="10.0" layoutY="70.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="170.0" text="Select All Cells" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Button onAction = "#selectNone" layoutX="10.0" layoutY="108.0" mnemonicParsing="false" text="Clear Selection" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Button onAction = "#invertSelection" layoutX="10.0" layoutY="146.0" mnemonicParsing="false" text="Invert Selection" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Separator layoutX="193.0" layoutY="-5.0" orientation="VERTICAL" prefHeight="650.0" prefWidth="0.0" />
								<Label layoutX="10.0" layoutY="243.0" text="(Primary) Sort cells by:" AnchorPane.leftAnchor="10.0" />
								<ComboBox fx:id = "primarySort" layoutX="10.0" layoutY="264.0" prefHeight="31.0" prefWidth="173.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />								
								<Button layoutX="10.0" layoutY="567.0" mnemonicParsing="false" onAction="#handleVisualizeCellsButton" text="Visualize Selected Cells" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<ComboBox fx:id = "secondarySort" layoutX="10.0" layoutY="377.0" prefHeight="31.0" prefWidth="173.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Label layoutX="10.0" layoutY="356.0" prefHeight="21.0" prefWidth="176.0" text="(Secondary) Sort cells by:" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Label layoutX="10.0" layoutY="488.0" text="# Selected Cells:" />
								<TextField fx:id = "selectionCount" layoutX="121.0" layoutY="483.0" prefHeight="31.0" prefWidth="65.0" AnchorPane.rightAnchor="10.0" editable="false"/>
								<ComboBox fx:id = "miniViewSelection" layoutX="10.0" layoutY="27.0" prefHeight="31.0" prefWidth="173.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Label layoutX="10.0" layoutY="6.0" text="Show in Mini View:" AnchorPane.leftAnchor="10.0" />
							</children>
						</AnchorPane>
					</left>
					<center>
						<ScrollPane BorderPane.alignment="CENTER" >
							<content>
								<TilePane fx:id = "tilePane" hgap="10.0" vgap="10.0">
									<padding>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</padding>
								</TilePane>
							</content>
						</ScrollPane>
					</center>
				</BorderPane>
			</content>
		</ScrollPane>
	</content>
</Tab>
