<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Tab closable="false" disable="true" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.materialize.MaterializeController">
	<graphic>
		<BorderPane>
			<center>
				<ImageView fx:id="image" pickOnBounds="true"
					preserveRatio="true" BorderPane.alignment="CENTER">
					<image>
						<Image url="images/gear_black.png" />
					</image>
				</ImageView>
			</center>
			<bottom>
				<Label text="Materialize" BorderPane.alignment="CENTER" />
			</bottom>
		</BorderPane>
	</graphic>
	<content>
		<ScrollPane>
			<content>
				<BorderPane>
					<left>
						<AnchorPane prefHeight="645.0" prefWidth="196.0"
							BorderPane.alignment="CENTER">
							<children>
								<Button layoutX="10.0" layoutY="500.0" mnemonicParsing="false"
									onAction="#exportSelected" text="Export Logs of Selected Cells"
									textAlignment="CENTER" wrapText="true" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" />
								<Button layoutX="10.0" layoutY="83.0" mnemonicParsing="false"
									onAction="#selectAll" prefHeight="31.0" prefWidth="170.0"
									text="Select All Cells" textAlignment="CENTER"
									AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Button layoutX="10.0" layoutY="121.0" mnemonicParsing="false"
									onAction="#selectNone" text="Clear Selection" textAlignment="CENTER"
									AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Button layoutX="10.0" layoutY="159.0" mnemonicParsing="false"
									onAction="#invertSelection" text="Invert Selection"
									textAlignment="CENTER" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0" />
								<Separator layoutX="193.0" layoutY="-5.0" orientation="VERTICAL"
									prefHeight="650.0" prefWidth="0.0" />
								<Label layoutX="11.0" layoutY="281.0" text="Sort cells by:"
									AnchorPane.leftAnchor="11.0" />
								<ComboBox fx:id="primarySort" layoutX="11.0" layoutY="302.0"
									prefHeight="31.0" prefWidth="173.0" AnchorPane.leftAnchor="11.0"
									AnchorPane.rightAnchor="9.0" />
								<Button layoutX="10.0" layoutY="567.0" mnemonicParsing="false"
									onAction="#handleVisualizeCellsButton" text="Do Process Mining with Selected Cells"
									textAlignment="CENTER" wrapText="true" AnchorPane.leftAnchor="10.0"
									AnchorPane.rightAnchor="10.0">
									<font>
										<Font name="System Bold" size="15.0" />
									</font>
								</Button>
								<Label layoutX="10.0" layoutY="464.0" text="# Selected Cells:" />
								<TextField fx:id="selectionCount" editable="false"
									layoutX="121.0" layoutY="459.0" prefHeight="31.0" prefWidth="65.0"
									AnchorPane.rightAnchor="10.0" />
								<ComboBox fx:id="miniViewSelection" layoutX="10.0"
									layoutY="27.0" prefHeight="31.0" prefWidth="173.0"
									AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Label layoutX="10.0" layoutY="6.0" text="Show cell mini views as:"
									AnchorPane.leftAnchor="10.0" />
								<CheckBox fx:id = "showOnlySelected" layoutX="11.0" layoutY="202.0"
									mnemonicParsing="false" prefHeight="29.0" prefWidth="176.0"
									text="Show only selected cells" wrapText="true">
									<font>
										<Font size="13.0" />
									</font>
								</CheckBox>
								<Button onAction="#updateCubeToMatchSelection" layoutX="34.0" layoutY="374.0" mnemonicParsing="false"
									prefHeight="57.0" prefWidth="131.0" text="Update Cube to Match Selection"
									textAlignment="CENTER" wrapText="true" />
								<CheckBox fx:id = "showEmpty" layoutX="11.0" layoutY="231.0"
									mnemonicParsing="false" prefHeight="29.0" prefWidth="176.0"
									text="Show empty cells" wrapText="true">
									<font>
										<Font size="13.0" />
									</font>
								</CheckBox>
							</children>
						</AnchorPane>
					</left>
					<center>
						<ScrollPane BorderPane.alignment="CENTER">
							<content>
								<TilePane fx:id="tilePane" hgap="10.0" vgap="10.0">
									<padding>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</padding>
								</TilePane>
							</content>
						</ScrollPane>
					</center>
				</BorderPane>
			</content>
		</ScrollPane>
	</content>
</Tab>
